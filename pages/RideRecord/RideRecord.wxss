page{
  background: #f7f7f7;
}
/*日期及当前消费 */

.Consumption_date {
  font-size: 30rpx;
  width: 100%;
  height: 100rpx;
  align-items: center;
  background: #fff;
}

/*日期  */

.date {
  width: 180rpx;
  height: 40rpx;
  color: #217ce7;
  /* border: 1rpx solid #aaa; */
  border-radius: 4rpx;
  text-align: center;
}

.date_start {
  margin-left: 40rpx;
}

.date_end {
  margin-left: 10rpx;
}

.Font {
  margin-left: 10rpx;
}

.money {
  margin-left: 14rpx;
}

/*记录  */

.take {
  width: 100%;
  height: 147rpx;
  margin-top: 26rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}

.time {
  width: 76%;
  margin-left: 40rpx;
}

/*乘车记录日期及支付状况  */

.RideDate {
  color: #217ce7;
  font-size: 30rpx;
  height: 74rpx;
  line-height: 74rpx;
}

/*机场大巴名  */

.BusName {
  width: 74%;
  margin-left: 40rpx;
}

/*机场大巴线路名称  */

.BUS {
  width: 100%;
  font-size: 30rpx;
  height: 74rpx;
  line-height: 74rpx;
}
